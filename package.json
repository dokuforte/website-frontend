    "axios": "^0.26.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-attrs": "^4.1.3",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.7",
    "object.fromentries": "^2.0.3"
  },
  "keywords": []
}
